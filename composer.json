{
    "name": "jazzman/autoload-interface",
    "authors": [
        {
            "name": "Vasyl Sokolyk",
            "email": "vsokolyk@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "require": {
        "php": ">=7.1"
    },
    "autoload": {
        "psr-4": {
            "JazzMan\\AutoloadInterface\\":""
        },
        "files": [
            "helper.php"
        ]
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    }
}
